{
  "name": "woonuxt",
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev --host",
    "dev:ssl": "NODE_TLS_REJECT_UNAUTHORIZED=0 nuxt dev --https --ssl-cert localhost.pem --ssl-key localhost-key.pem",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "serve": "nuxi generate && npx serve .output/public -l 8080"
  },
  "dependencies": {
    "@stripe/stripe-js": "^4.8.0",
    "@vueform/slider": "^2.1.10",
    "@vueuse/core": "^11.1.0",
    "@nuxt/image": "^1.8.0",
    "@nuxtjs/google-fonts": "^3.2.0",
    "@nuxtjs/sitemap": "^6.1.1",
    "nuxt-mail": "^5.1.1"
  },
  "devDependencies": {
    "@nuxt/icon": "^1.5.5",
    "@nuxtjs/i18n": "^8.5.5",
    "@tailwindcss/typography": "^0.5.15",
    "@tailwindcss/forms": "^0.5.9",
    "nuxt": "^3.13.2",
    "nuxt-graphql-client": "^0.2.36",
    "prettier": "^3.3.3",
    "woonuxt-settings": "^1.0.61",
    "nuxt-phosphor-icons": "^1.3.0",
    "tailwindcss": "^3.4.13"
  },
  "overrides": {
    "sharp": "0.32.6"
  }
